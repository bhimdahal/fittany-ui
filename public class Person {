public class Person {
	
	private char middleNameInitial;
	private String firstName;
	private int age;
	private long population;
	private float weight;
	private double salary;

	Person(){};
	Person(char middleNameInitial, String firstName){
			this.middleNameInitial = middleNameInitial;
			this.firstName = firstName;
	}
	Person(char middleNameInitial, String firstName, int age){
			this.middleNameInitial = middleNameInitial;
			this.firstName = firstName;
			this.age = age;

	}
	Person(char middleNameInitial, String firstName, int age, long population, float weight, double salary){
			this.middleNameInitial = middleNameInitial;
			this.firstName = firstName;
			this.age = age;
			this.population = population;
			this.weight = weight;
			this.salary = salary;

	}

	public char getMiddleNameInitial(){
		return this.middleNameInitial;
	}

	public void getMiddleNameInitial(char middleNameInitial){
		 this.middleNameInitial = middleNameInitial;
	}


}